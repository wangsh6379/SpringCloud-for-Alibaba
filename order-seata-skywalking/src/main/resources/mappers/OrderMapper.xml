<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wangsh.spirngcloudforalibaba.dao.OrderDao">

    <insert id="insertOrder" parameterType="com.wangsh.spirngcloudforalibaba.entity.Order">
        INSERT INTO `order_tb1`
